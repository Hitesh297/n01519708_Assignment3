@model n01519708_assignment3_w2022.Models.Teacher

@{
    ViewBag.Title = "DeleteConfirm";
}

<div id="containershow">
    <nav>
        <a class="navlink" href="/Teacher/Show/@Model.TeacherId">Cancel</a>
    </nav>
    <h2>Are you sure you want to delete @Model.FirstName @Model.LastName?</h2>
    <p>This action cannot be undone.</p>
    <form method="POST" action="/Teacher/Delete/@Model.TeacherId">
        <button class="btn btn-danger">
            Confirm Delete
        </button>
    </form>
</div>


